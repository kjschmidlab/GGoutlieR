    stats4, 
    FastKNN,
    foreach,
    doParallel,
    iterators,
    parallel,
    rworldmap,
    scales,
    plotrix,
    mapplots, 
    RColorBrewer,
    rworldxtra,
    dichromat,
    sp
Encoding: UTF-8
LazyData: true
